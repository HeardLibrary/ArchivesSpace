.logo {
	display: block;
  	-moz-box-sizing: border-box;
  	box-sizing: border-box;
  	background: url(/assets/images/heard-logo.svg) no-repeat;
  	width: 255px; /* Width of new image */
  	height: 100px; /* Height of new image */
  	padding-left: 255px; /* Equal to width of new image */
}

